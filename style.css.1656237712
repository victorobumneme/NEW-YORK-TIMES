body {
  background-color: #fff;
  font-family: Arial, Helvetica, sans-serif;
  color: black;
  padding: 0;
  margin: 0;
  width: 100%;
}

header {
  display: flex;
  background-color: #333;
  padding: 3px;
  color: #fff;
}

.navbar {
  position: fixed;
  top: 0;
  background: white;
  height: 50px;
  width: 100%;
  border-bottom: 1px solid #e2e2e2;
  z-index: 100;
  display: flex;
  justify-content: space-between;
  padding-top: 10px;
  margin-left: 20px;
}

.nav_icon {
  margin-left: 20px;
}

.nav_btn {
  padding: 8px;
  background-color: #326891;
  color: #fff;
  font-size: 14px;
  font-weight: bold;
  border-radius: 4px;
  border: none;
  margin-left: 2px;
}

h2 {
  font-weight: 700;
  font-style: italic;
  font-size: 40px;
  margin-left: 350px;
  font-family: Georgia, 'Times New Roman', Times, serif;
  color: #121212;
  line-height: 48px;
  width: 700px;
}

.icon {
  margin-left: 500px;
  margin-top: 0;
}

.img {
  width: 60%;
  margin-left: 280px;
}

.img3 {
  width: 50%;
  margin-left: 350px;
}

.main {
  margin-top: 80px;
}

#first_p {
  font-size: 15px;
  width: 60%;
  font-style: normal;
  font-weight: 400;
  color: #666;
  font-family: Georgia, 'Times New Roman', Times, serif;
  margin-left: 280px;
  margin-top: 0;
}

.img2 {
  width: 100%;
}

.iframe {
  margin-left: 30%;
}

p {
  font-size: 20px;
  width: 50%;
  font-style: normal;
  font-weight: 400;
  color: #333;
  font-family: Georgia, 'Times New Roman', Times, serif;
  margin-left: 350px;
  line-height: 25px;
}

#second_p {
  font-size: 16px;
  font-style: normal;
  font-weight: 400;
  color: #333;
  font-family: Georgia, 'Times New Roman', Times, serif;
  margin-left: 0;
  margin-top: 0;
}

h4 {
  font-weight: 600;
  font-size: 23px;
  margin-left: 400px;
}

h6 {
  font-weight: bold;
  font-family: sans-serif;
  font-size: 14px;
  text-decoration: none;
}

.editors-pick {
  float: right;
  width: 250px;
  background: white;
  position: absolute;
  right: 0;
  margin-right: 10px;
  margin-left: auto;
}

.editor-item {
  display: flex;
  margin-top: 20px;
}

.editor-img {
  width: 150px;
  height: 100px;
}

.editor-p {
  margin-left: 10px;
  font-size: 14px;
  width: 50%;
}

.flex1-item1 {
  vertical-align: top;
  display: inline-block;
  text-align: center;
  width: 360px;
}

.flex1-image {
  width: 350px;
  height: 250px;
}

/* .flex1-link {

} */

.footer {
  width: 100%;
  margin-left: 0;
}

.footer-home {
  display: flex;
  justify-content: space-between;
  margin: 30px;
}

#footer-go {
  text-decoration: none;
  margin-right: 80px;
}

.sub-footer {
  display: flex;
  align-content: center;
  margin: 50px;
  justify-content: space-evenly;
}

h3 {
  font-style: normal;
  font-weight: 700;
  font-size: 13px;
  font-family: Arial, Helvetica, sans-serif;
  line-height: 15px;
  color: #333;
}

.footer_link {
  color: black;
  font-size: 12px;
  text-decoration: none;
  margin-right: 30px;
}

.last_part {
  /* margin-left: 100px;
  width: 100%; */
}

.last_part a {
  color: #97667f;
  font-size: 12px;
  text-decoration: none;
  font-family: calibri;
}

.sideright {
  float: right;
  width: 300px;
  margin-right: 10px;
  font-size: 10px;
  border-left: 1px solid #9a9a9a;
}

hr {
  width: 70%;
}

.sidebar {
  float: right;
  width: 40px;
}

.side_icon {
  margin-left: 15px;
  font-size: 16px;
  font-weight: 700;
  line-height: 15px;
  font-style: normal;
  color: #333;
  text-decoration: none;
}

.sub {
  margin-left: 15px;
}

.side-small {
  font-style: normal;
  font-size: 11px;
  line-height: 21px;
  font-family: Arial, Helvetica, sans-serif;
  font-weight: 500;
  color: #000;
  margin-left: 15px;
  text-decoration: none;
  margin-bottom: 5px;
}

.side-smallest {
  margin-left: 15px;
  font-size: 11px;
  font-weight: 700;
  line-height: 21px;
  font-style: normal;
  color: #000;
  text-decoration: none;
}
